{
    "manifest_version": 3,
    "name": "Custom News Fetcher",
    "version": "1.0",
    "description": "Fetches news similar to Instagram meme pages for selected topics.",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  